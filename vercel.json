{
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "https://cijene-me-api.onrender.com/auth/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://cijene-me-api.onrender.com/:path*",
      "has": [
        {
          "type": "header",
          "key": "Authorization"
        }
      ]
    },
    {
      "source": "/api/:path*/",
      "destination": "https://cijene-me-api.onrender.com/:path*/",
      "has": [
        {
          "type": "header",
          "key": "Authorization"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
} 